{
  "schema_version": 1,
  "mixin": [
    "caravanpackrat.mixins.json",
    "caravanpackrat-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "caravanpackrat",
    "version": "${version}",
    "metadata": {
      "name": "Caravan Pack Rat",
      "description": "Hey there pack rat, you! Give us your items and we'll hold on to them for ya.",
      "contributors": {
        "Me!": "Owner"
      },
      "contact": {
        "sources": "https://github.com/architectury/architectury-templates"
      },
      "icon": "assets/caravanpackrat/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.caravanpackrat.quilt.CaravanPackRatQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.1.12"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}